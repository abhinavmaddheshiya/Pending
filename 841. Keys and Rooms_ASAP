class Solution {
public:
    void dfstraverse(vector<vector<int>>& rooms, int temp, vector<int>& visited){
        visited[temp]=1;
        for(int i:rooms[temp]){
            if(visited[i]==-1)dfstraverse(rooms, i, visited);
        }
    }
    bool canVisitAllRooms(vector<vector<int>>& rooms) {
        int size=rooms.size();
        vector<int> visited(size, -1);
        dfstraverse(rooms, 0, visited);
        for(int i=0;i<size;i++){
            if(visited[i]==-1)return false;
        }
        return true;
    }
};
